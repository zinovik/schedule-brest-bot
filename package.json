{
  "name": "schedule-brest-bot",
  "version": "0.1.0",
  "description": "simple telegram bot",
  "main": "index.ts",
  "dependencies": {
    "axios": "^0.19.0",
    "babel-polyfill": "^6.26.0",
    "dotenv": "^8.0.0",
    "jsdom": "^15.1.1",
    "redis": "^2.8.0"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.3.3",
    "@types/aws-lambda": "^8.10.25",
    "@types/dotenv": "^6.1.1",
    "@types/jest": "^24.0.13",
    "@types/jsdom": "^12.2.3",
    "@types/node": "^12.0.3",
    "@types/redis": "^2.8.13",
    "jest": "^24.8.0",
    "netlify-lambda": "^1.4.13",
    "nodemon": "^1.19.1",
    "prettier": "^1.17.1",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.2.0",
    "tslint": "^5.16.0",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "^3.4.5"
  },
  "scripts": {
    "dev": "nodemon",
    "hello": "yarn dev src/lambda/hello",
    "icebrest": "yarn dev src/lambda/icebrest",
    "dvvsbrest": "yarn dev src/lambda/dvvsbrest",
    "check-schedules": "yarn dev src/lambda/check-schedules",
    "start:lambda": "netlify-lambda serve src/lambda --config ./webpack.functions.js",
    "build:lambda": "netlify-lambda build src/lambda --config ./webpack.functions.js",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "test": "jest --config jestconfig.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zinovik/schedule-brest-bot.git"
  },
  "author": "",
